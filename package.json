{
	"name": "metadata-extractor",
	"version": "2.0.0",
	"description": "Plugin that extracts metadata and saves it in JSON format",
	"main": "src/main.ts",
	"type": "module",
	"scripts": {
		"dev": "cross-env BUILD=dev node esbuild.mjs",
		"build": "cross-env BUILD=production node esbuild.mjs",
		"release": "standard-version",
		"test": "jest",
		"format": "npx prettier --write src/",
		"lint": "eslint ."
	},
	"standard-version": {
		"t": "",
		"types": [
			{
				"type": "perf",
				"hidden": false,
				"section": "Performance"
			},
			{
				"type": "feat",
				"hidden": false,
				"section": "Features"
			},
			{
				"type": "fix",
				"hidden": false,
				"section": "Bug Fixes"
			}
		],
		"releaseCommitMessageFormat": "Metadata Extractor: {{currentTag}}"
	},
	"keywords": [],
	"author": "twio142",
	"license": "MIT",
	"devDependencies": {
		"@types/eslint": "^9.6.1",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.10.1",
		"@typescript-eslint/eslint-plugin": "^8.48.1",
		"@typescript-eslint/parser": "^8.48.1",
		"cross-env": "^10.1.0",
		"esbuild": "0.27.1",
		"esbuild-plugin-inline-worker": "^0.1.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"globals": "^16.5.0",
		"jest": "^30.2.0",
		"obsidian": "^1.10.3",
		"prettier": "3.7.4",
		"standard-version": "^9.5.0",
		"ts-jest": "^29.4.6",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3"
	}
}
